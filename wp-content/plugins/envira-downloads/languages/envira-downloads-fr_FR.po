msgid "Top Left"
msgstr "En haut à gauche"

#: includes/global/common.php:95
msgid "Top Right"
msgstr "En haut à droite"

#: includes/global/common.php:96
msgid "Bottom Left"
msgstr "En bas à gauche"

#: includes/global/common.php:97
msgid "Bottom Right"
msgstr "En bas à droite"

#: includes/global/common.php:114
msgid "Above Gallery"
msgstr "Au-dessus de la galerie"

#: includes/global/common.php:115
msgid "Below Gallery"
msgstr "En-dessous de la galerie"

#: includes/global/common.php:138
msgid "Default"
msgstr "Par défaut"

#: includes/global/common.php:173
msgid "Original Size"
msgstr "Taille originale"

#: includes/global/shortcode.php:134 includes/global/shortcode.php:308
msgid "Please enter the password to download this image."
msgstr "Veuillez saisir le mot de passe pour télécharger cette image."

#: includes/global/shortcode.php:435 includes/global/shortcode.php:457
msgid "Click here to download this image"
msgstr "Cliquez ici pour télécharger cette image"

#: includes/global/shortcode.php:435 includes/global/shortcode.php:457
msgid "Download"
msgstr "Journal des téléchargements"

